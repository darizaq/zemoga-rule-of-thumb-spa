    "prerender": "ng run zemoga-rule-of-thumb-spa:prerender",
    "e2e": "tsc --project e2e/tsconfig.json && nightwatch",
    "build:docker": "npm run build:ssr && docker build . -t darizaq/zemoga-rule-of-thumb-spa",
    "lint:ng": "ng lint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:ng && npm run lint:scss",
    "prepare": "husky install"
  },
  "private": true,
  "author": "Danilo Ariza",
  "repository": {
    "type": "git",

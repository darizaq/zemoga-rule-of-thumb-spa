  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --watch=true --code-coverage",
    "dev:ssr": "ng run zemoga-rule-of-thumb-spa:serve-ssr",
    "build:ssr": "npm run build && ng run zemoga-rule-of-thumb-spa:server:production",
